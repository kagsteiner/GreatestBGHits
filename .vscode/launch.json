    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Debug Game Core",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\scripts\\test-game-core.js",
            "args": [
                "game1.json"
            ],
            "console": "integratedTerminal"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Server",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\server.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Debug Match Analysis",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\scripts\\test-match-analysis.js",
            "args": [
                "game1.json"
            ],
            "console": "integratedTerminal"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Debug Current File",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${file}",
            "console": "integratedTerminal"
        }
    ]
}